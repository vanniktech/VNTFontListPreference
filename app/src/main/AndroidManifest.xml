<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.vanniktech.vntfontlistpreference.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.vanniktech.vntfontlistpreference.sample.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.vanniktech.vntfontlistpreference.sample.SettingsActivity"
            android:parentActivityName="com.vanniktech.vntfontlistpreference.sample.MainActivity">
        </activity>
    </application>

</manifest>